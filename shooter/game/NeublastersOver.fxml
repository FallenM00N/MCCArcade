            <Button fx:id="playButton" mnemonicParsing="false" onAction="#start" prefHeight="50.0" prefWidth="150.0" text="Play Again">
               <VBox.margin>
                  <Insets bottom="20.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="titleButton" mnemonicParsing="false" onAction="#titleScreen" prefHeight="50.0" prefWidth="150.0" text="Return to Title Screen">
               <VBox.margin>
                  <Insets bottom="20.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="quitButton" mnemonicParsing="false" onAction="#exit" prefHeight="50.0" prefWidth="150.0" text="Quit">
               <VBox.margin>
                  <Insets bottom="20.0" />
               </VBox.margin></Button>
         </children>
      </VBox>
   </bottom>
   <center>
      <Label fx:id="scoreLabel" alignment="CENTER" contentDisplay="CENTER" prefHeight="42.0" prefWidth="233.0" text="Score" BorderPane.alignment="CENTER">
         <font>
            <Font size="30.0" />
         </font>
      </Label>
   </center>
</BorderPane>
